{
	"name": "Lisk",
	"version": "0.9.1",
	"icons": {
		"128": "favicon-large.png",
		"16": "favicon.png"
	},
	"options_page": "options.html",
	"manifest_version": 2,
	"description": "Access the lisk.support tools or check your account or even send payments with nano",
	"permissions": [
		"contextMenus",
		"notifications",
		"storage"
	],
	"background": {
		"scripts": [
			"script.js"
		]
	},
	"browser_action": {
		"default_icon": "favicon.png",
		"default_popup": "popup.html"
	}
}
